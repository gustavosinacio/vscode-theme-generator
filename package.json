{
  "name": "vscode-ts-theme-generator",
  "version": "1.0.0",
  "author": "Gustavo Inacio",
  "description": "use typescript to create theme files for vscode",
  "main": "index.ts",
  "license": "ISC",
  "scripts": {
    "dev": "nodemon --watch '*.ts' --exec 'ts-node index.ts' -e ts"
  },
  "nodemonConfig": {
    "verbose": true
  },
  "devDependencies": {
    "@types/node": "^17.0.33",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "nodemon": "^2.0.16"
  }
}
